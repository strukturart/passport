{
  "name": "passport",
  "id": "passport",
  "description": "With parrot you can expand and maintain the vocabulary of the predictive text",

  "lang": "en-US",
  "start_url": "/index.html",
  "priority": "high",
  "userAgentInfo": "parrot written by strukturart@gmail.com",

  "icons": [
    {
      "src": "/assets/icons/icon-56-56.png",
      "type": "image/png",
      "sizes": "56x56"
    },
    {
      "src": "/assets/icons/icon-112-112.png",
      "type": "image/png",
      "sizes": "112x112"
    }
  ],

  "b2g_features": {
    "version": "0.0.708",
    "id": "parrot",
    "core": true,
    "categories": ["utilities"],
    "type": "privileged",
    "display": "fullscreen",
    "theme_color": "#ffffff",

    "developer": {
      "name": "strukturart",
      "url": "https://github.com/strukturart/parrot"
    },

    "dependencies": {
      "ads-sdk": "1.5.8"
    },

    "permissions": {
      "mobiledata": {},
      "wifidata": {},
      "calllog": {},
      "device-storage:sdcard": {
        "description": "Read/Write from/to sd-card",
        "access": "readwrite"
      },

      "video-capture": {
        "description": "Reading Codes using the Camera"
      },

      "spatialnavigation-app-manage": {
        "navigator.spatialNavigationEnabled": false
      }
    }
  }
}
